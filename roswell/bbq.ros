#!/bin/sh
#|-*- mode:lisp -*-|#
#| <Put a one-line description here>
exec ros -Q -- $0 "$@"
|#
(progn ;;init forms
  (ros:ensure-asdf)
  #+quicklisp (ql:quickload :bbq :silent t))

(defun main (&rest argv)
  (declare (ignorable argv))
  (let ((query (car argv)))
    (if (or (not query) (string-equal query ""))
        (bbq::mpc-clear-play-all)
        (bbq::mpc-clear-play (bbq::search-basic query)))))
